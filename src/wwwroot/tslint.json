{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "no-console": false,
        "interface-name": false,
        "quotemark": {
            "options": [
                "single",
                "jsx-double"
            ]
        },
        "no-shadowed-variable": false
    },
    "rulesDirectory": []
}
